{
  "version": 2,
  "name": "drqsatoshin-legal-pages",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/TOS",
      "dest": "/TOS/index.html"
    },
    {
      "src": "/TOS.html",
      "dest": "/TOS/index.html"
    },
    {
      "src": "/Policy",
      "dest": "/Policy/index.html"
    },
    {
      "src": "/Policy.html",
      "dest": "/Policy/index.html"
    }
  ]
}
